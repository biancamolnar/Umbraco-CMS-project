@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "_Layout.cshtml";
    ViewData["PageTitle"] = @Model.Value("pageTitle");
	ViewData["PageDescription"] = @Model.Value("pageDescription");
	ViewData["PageKeywords"] = @Model.Value("pageKeywords");
}

 @await Html.PartialAsync("_NavSection")

 @await Html.PartialAsync("_ShowcaseSection")
 
 @await Html.PartialAsync("_BrandSection")
 
  <!-- features -->
  <section class="features">
    <div class="container">
      <div class="features-content">
        <h6>@Model.Value("featuresSectionTitle")</h6>
        <h2>@Model.Value("featuresSectionHeading")</h2>
        <a class="btn-yellow a" href="@Model.Value("featuresSectionButtonUrl")">@Model.Value("featuresSectionButtonText") <i class="fa-regular fa-arrow-up-right"></i></a>
      </div>
      <div class="features-services">

            @Html.GetBlockListHtml(Model.FeaturesSection)
            
      </div>
    </div>
  </section>
  
 @await Html.PartialAsync("_AboutSection")

  <!-- services -->
  <section class="services">
    <div class="container">
      <h6>@Model.Value("servicesSectionTitle")</h6>
      <h2>@Model.Value("servicesSectionHeading")</h2>
      
            @Html.GetBlockListHtml(Model.Services)
      
      <div class="btn-center-bottom">
        <a class="btn-transparent" href="@Model.Value("servicesSectionButtonUrl")">@Model.Value("servicesSectionButtonText") <i class="fa-regular fa-arrow-up-right"></i></a>
      </div>
    </div>
    <img src="~/images/background-effects/white-waves-right.svg" alt="">
  </section> 
  
 @await Html.PartialAsync("_ChooseUsSection")

 @await Html.PartialAsync("_ProjectsSection")

 @await Html.PartialAsync("_TeamMembersSection")
 
 @await Html.PartialAsync("_TestimonialSection")
 
 @await Html.PartialAsync("_ArticleNewsSection")
 
 @await Html.PartialAsync("_NewsletterSection")